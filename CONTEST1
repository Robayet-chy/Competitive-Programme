#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cin >> n;

    
    vector<int> primeFactorsCount(n + 1, 0);

   
    for (int i = 2; i <= n; i++) {
        if (primeFactorsCount[i] == 0) {
            
            for (int j = i; j <= n; j += i) {
                primeFactorsCount[j]++;
            }
        }
    }

    
    int count = 0;
    for (int i = 1; i <= n; i++) {
        if (primeFactorsCount[i] == 2) {
            count++;
        }
    }

    cout << count << endl;
    return 0;
}
